{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./publish",
    "removeComments": true,
    "sourceMap": false,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "rootDir": "./",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  },
  // "files": [
  //   "./dist/**",
  // ],
  // "include": [
  //   "./dist/**",
  // ],
  "exclude": [
    "./src",
  ]
}
